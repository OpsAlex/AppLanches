@model Lanche


    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="img-thumbnail">
            <img src="@Model.ImagemUrl" class="img-rounded" alt="" />
        </div>

        <div class="text-capitalize">
            <h3 class="pull-right text-info"> @Model.Preco.ToString("c")</h3>
            <h3>@Model.Nome</h3>
            <p>@Model.DescricaoCurta</p>
        </div>
    </div>
